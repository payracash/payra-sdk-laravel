{
  "name": "payracash/payra-sdk-laravel",
  "description": "Payra Crypto Payments signature generator SDK for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Wraith",
      "email": "contact@payra.cash"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0|^11.0",
    "web3p/web3.php": "^1.0",
    "web3p/ethabi": "^0.1",
    "web3p/utils": "^0.1",
    "web3p/rlp": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Payra\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Payra\\PayraServiceProvider"
      ],
      "aliases": {
        "Payra": "Payra\\Facades\\Payra"
      }
    }
  }
}
